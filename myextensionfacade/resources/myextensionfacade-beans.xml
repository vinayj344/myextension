<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="org.myextensionfacade.MyProductData">
		<property name="code" type="java.lang.String"></property>
		<property name="name" type="java.lang.String"></property>
		<property name="manufacturerName" type="java.lang.String"></property>
	</bean>

	<bean class="org.myextensionfacade.MyCustomerData">
		<property name="firstname" type="java.lang.String"></property>
		<property name="lastname" type="java.lang.String"></property>
		<property name="gender" type="java.lang.String"></property>
		<property name="email" type="java.lang.String"></property>
		<property name="password" type="java.lang.String"></property>
		<property name="phone" type="java.lang.String"></property>
		<property name="address" type="java.lang.String"></property>
	</bean>

</beans>
